# Railway Environment Variables
# These will be automatically set by Railway when you add a PostgreSQL service

# Database (automatically set by Railway PostgreSQL service)
# DATABASE_URL=postgresql://...

# Application settings
PORT=8000
LOG_LEVEL=INFO

# RSS and scraping
RSS_URL=https://omny.fm/shows/daniel-and-kellys-extraordinary-universe/playlists/podcast.rss
USER_AGENT=Podcast Search Bot 1.0

# Embedding model
MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2

# Chunking parameters
CHUNK_SIZE=1200
CHUNK_OVERLAP=200

# Search parameters
TOP_K=10
HYBRID_ALPHA=0.7

